{
  "api": "http://localhost:8080",
  "webui": {
    "enable": true,
    "host": "localhost:8181"
  },
  "fetch": {
    "save_path_default": "./downloads",
    "save_paths": {
      "mp3$": "./podcasts"
    }
  },
  "sort": {
    "save_paths": {
      "rss": "./podcast"
    },
    "save_path_default": "./sorted",
    "search_paths": [
      "./downloads"
    ],
    "providers_to_move": [
      "roosterteeth"
    ]
  }
}
